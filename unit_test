import org.junit.Test;
import static org.junit.Assert.*;

public class unit_test {
    @Test
    public void testGenerateAnswer() {
        MyUnit myUnit = new MyUnit();
        guessing_game.generateAnswer();
        assertTrue(guessing_game.generateAnswer>=1);
        assertTrue(guessing_game.generateAnswer<=25);
    }
}

public class unit_test {
    @Test
    public void testCheckGuess() {
        MyUnit myUnit = new MyUnit();
        guessing_game.checkGuess();
        if (guessing_game.answer==guessing_game.guess){
            assertEquals(guessing_game.answer,guessing_game.guess);
        }
        
    }
}







